{
    "$schema": "https://json.schemastore.org/prettierrc",
    "trailingComma": "es5",
    "semi": false,
    "tabWidth": 4,
    "useTabs": true,
    "singleQuote": true,
    "printWidth": 100,
    "singleAttributePerLine": true,
    "vueIndentScriptAndStyle": false,
    "arrowParens": "always",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "htmlWhitespaceSensitivity": "ignore",
    "endOfLine": "lf"
}